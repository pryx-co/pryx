    {
      "id": "filesystem",
      "name": "File System",
      "description": "Access and manage local files and directories with secure path restrictions",
      "category": "filesystem",
      "author": "Anthropic",
      "repository": "https://github.com/modelcontextprotocol/servers",
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-filesystem"]
      },
      "capabilities": {
        "tools": [
          {
            "name": "read_file",
            "description": "Read contents of a file"
          },
          {
            "name": "write_file",
            "description": "Write contents to a file"
          },
          {
            "name": "list_directory",
            "description": "List contents of a directory"
          }
        ]
      },
      "settings": {
        "requiresArgs": true,
        "argDescription": "Allowed directories (space-separated paths)"
      }
    },
